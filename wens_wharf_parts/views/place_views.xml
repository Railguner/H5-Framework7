<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="ws_wharf_parts_place_list" model="ir.ui.view">
            <field name="name">仓位列表</field>
            <field name="model">ws_wharf_parts_place</field>
            <field name="arch" type="xml">
                <tree string="仓位列表">
                    <field name="place_name"/>
                    <field name="place_description"/>
                </tree>
            </field>
        </record>

        <record id="ws_wharf_parts_place_form" model="ir.ui.view">
            <field name="name">仓位列表详情</field>
            <field name="model">ws_wharf_parts_place</field>
            <field name="arch" type="xml">
                <form string="仓位列表详情">
                    <sheet>
                        <group>
                            <field name="place_name"/>
                            <field name="place_description"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="action_ws_wharf_parts_place_list" model="ir.actions.act_window">
            <field name="name">仓位列表</field>
            <field name="res_model">ws_wharf_parts_place</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="ws_wharf_parts_place_list"/>
        </record>
    </data>
</odoo>